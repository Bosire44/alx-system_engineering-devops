#!/bin/bash

# Read input from stdin
read -p "Enter a list of words (space-separated): " input

# Convert input into an array
words=($input)

# Create an associative array to track word counts
declare -A wordCount

# Count the occurrences of each word
for word in "${words[@]}"; do
    ((wordCount[$word]++))
done

# Print words that appear exactly once
for word in "${words[@]}"; do
    if [ "${wordCount[$word]}" -eq 1 ]; then
        echo "$word"
    fi
done

